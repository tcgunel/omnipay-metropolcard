{
    "name": "tcgunel/omnipay-metropolcard",
    "type": "library",
    "description": "Omnipay extension for MetropolCard",
    "keywords": [
        "tokenflex",
        "omnipay",
        "ödeme",
        "otp",
        "qr"
    ],
    "homepage": "https://github.com/tcgunel/omnipay-metropolcard",
    "license": "MIT",
    "authors": [
        {
            "name": "Tolga Can Günel",
            "email": "tlgcngnl@gmail.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/tcgunel/omnipay-metropolcard/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\MetropolCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\MetropolCard\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "league/omnipay": "^3",
        "ext-json": "*",
        "ext-openssl": "*",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "omnipay/tests": "^4",
        "brianium/paratest": "*",
        "fzaninotto/faker": "*",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "scripts": {
        "test": "vendor/bin/paratest --processes 6",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
